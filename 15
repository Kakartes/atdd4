programa
{
	funcao inicio()
	{
    inteiro base
    inteiro expoente
    inteiro resultado
    inteiro contador
    escreva("Digite a base: ")
    leia(base)
    escreva("Digite o expoente: ")
    leia(expoente)
    resultado = 1
    contador = 0
    enquanto (contador < expoente ){
        resultado = resultado * base
        contador = contador + 1
    }
    escreva("Resultado: ", resultado)
	}
}
