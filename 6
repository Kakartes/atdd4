programa {
  funcao inicio() 
  {
        inteiro comprimento
         inteiro largura
       inteiro area
       inteiro areax2
  
    comprimento = 105
    largura = 68

    area = comprimento * largura

    escreva (area)
    escreva ("\n")
    escreva (area * 2)
    areax2 = area * 2
    escreva ("\n")

    escreva (areax2*100)

    
  }
}
